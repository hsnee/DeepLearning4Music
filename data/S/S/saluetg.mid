<SCRIPT>

var selected = 0;
var current = 30;

function CountEm(form, filenumber)
{
	filenumber = filenumber + 2
	if (form.elements[filenumber].checked)
	{
		if (selected >= current)
		{
			alert("Sorry.  You may only zip 30 files at a time.")
			form.elements[filenumber].checked = false;
		}
		else
		{
			selected = selected + 1;
			form.counter.value = selected;
		}
	}
	else
	{
		selected = selected - 1;
		form.counter.value = selected;
	}
}




